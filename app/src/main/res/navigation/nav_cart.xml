<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_cart"
    app:startDestination="@id/cartFragment">

    <fragment
        android:id="@+id/cartFragment"
        android:name="com.angiedev.sheystore.ui.cart.view.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" >
        <action
            android:id="@+id/action_cartFragment_to_checkoutFragment"
            app:destination="@id/checkoutFragment" />
    </fragment>
    <fragment
        android:id="@+id/checkoutFragment"
        android:name="com.angiedev.sheystore.ui.checkout.view.CheckoutFragment"
        android:label="fragment_checkout"
        tools:layout="@layout/fragment_checkout" >
        <action
            android:id="@+id/action_checkoutFragment_to_choiceMyShippingAddressFragment"
            app:destination="@id/choiceMyShippingAddressFragment" />
    </fragment>
    <fragment
        android:id="@+id/choiceMyShippingAddressFragment"
        android:name="com.angiedev.sheystore.ui.choiceMyShippingAddress.view.ChoiceMyShippingAddressFragment"
        android:label="fragment_choice_my_shipping_address"
        tools:layout="@layout/fragment_choice_my_shipping_address" />
</navigation>