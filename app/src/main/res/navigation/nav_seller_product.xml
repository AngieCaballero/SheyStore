<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_seller_product"
    app:startDestination="@id/productsFragment">

    <fragment
        android:id="@+id/productsFragment"
        android:name="com.angiedev.sheystore.ui.modules.seller.products.view.ProductsFragment"
        android:label="ProductsFragment"
        tools:layout="@layout/fragment_products">
        <action
            android:id="@+id/to_createProductFragment"
            app:destination="@id/createProductFragment" />
    </fragment>

    <fragment
        android:id="@+id/createProductFragment"
        android:name="com.angiedev.sheystore.ui.modules.seller.products.view.CreateProductFragment"
        android:label="CreateProductFragment"
        tools:layout="@layout/fragment_create_product"/>
</navigation>